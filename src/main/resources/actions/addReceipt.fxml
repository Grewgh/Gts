<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Actions.Add_receipt">
   <children>
      <Label fx:id="label" layoutX="10.0" layoutY="10.0" text="Выберите договор:">
         <font>
            <Font name="Cambria" size="16.0" />
         </font>
      </Label>
      <Button fx:id="saveButton" layoutX="10.0" layoutY="150.0" mnemonicParsing="false" onAction="#handleSave" text="Сохранить" />
      <Label fx:id="label2" layoutX="10.0" layoutY="40.0" text="Дача начисления:">
         <font>
            <Font name="Cambria" size="16.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboBox" layoutX="180.0" layoutY="10.0" prefWidth="150.0" />
      <DatePicker fx:id="datePicker" layoutX="180.0" layoutY="40.0" />
   </children>
</AnchorPane>
