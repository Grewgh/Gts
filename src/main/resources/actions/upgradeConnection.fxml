<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Actions.Upgrade_connection">
   <children>
      <Label fx:id="label" layoutX="10.0" layoutY="10.0" text="Дата подключения:">
         <font>
            <Font name="Cambria" size="16.0" />
         </font>
      </Label>
      <Label fx:id="label2" layoutX="10.0" layoutY="40.0" text="Дата отключения:">
         <font>
            <Font name="Cambria" size="16.0" />
         </font>
      </Label>
      <Label fx:id="label3" layoutX="10.0" layoutY="70.0" text="Укажите квартиру:">
         <font>
            <Font name="Cambria" size="16.0" />
         </font>
      </Label>
      <Label fx:id="label4" layoutX="10.0" layoutY="100.0" text="Укажите тип телефона:">
         <font>
            <Font name="Cambria" size="16.0" />
         </font>
      </Label>
      <Label fx:id="label5" layoutX="10.0" layoutY="130.0" text="Укажите статус:">
         <font>
            <Font name="Cambria" size="16.0" />
         </font>
      </Label>
      <Label fx:id="label6" layoutX="10.0" layoutY="160.0" text="Укажите номер:">
         <font>
            <Font name="Cambria" size="16.0" />
         </font>
      </Label>
      <Label fx:id="label7" layoutX="10.0" layoutY="190.0" text="Укажите клиента:">
         <font>
            <Font name="Cambria" size="16.0" />
         </font>
      </Label>
      <DatePicker fx:id="datePicker" layoutX="250.0" layoutY="10.0" />
      <DatePicker fx:id="datePicker2" layoutX="250.0" layoutY="40.0" />
      <ComboBox fx:id="combobox" layoutX="250.0" layoutY="70.0" prefWidth="150.0" />
      <ComboBox fx:id="combobox2" layoutX="250.0" layoutY="100.0" prefWidth="150.0" />
      <ComboBox fx:id="combobox3" layoutX="250.0" layoutY="130.0" prefWidth="150.0" />
      <ComboBox fx:id="combobox4" layoutX="250.0" layoutY="160.0" prefWidth="150.0" />
      <ComboBox fx:id="combobox5" layoutX="250.0" layoutY="190.0" prefWidth="150.0" />
      <Button fx:id="saveButton" layoutX="10.0" layoutY="250.0" mnemonicParsing="false" onAction="#handleUpgrade" text="Сохранить" />
   </children>
</AnchorPane>
